<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_stock_quant_tree_inherit_value" model="ir.ui.view">
            <field name="name">view.stock.quant.tree.inherit.value</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
