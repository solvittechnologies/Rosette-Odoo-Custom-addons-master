<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="custom_favicon_fav_ico_id" inherit_id="web.layout" name="Fastras Icon">
            <xpath expr="//html/head/link[1]" position="replace">
                    <link type="image/x-icon" rel="shortcut icon"
                          href="/custom_favicon/static/src/img/favicon.ico"/>
            </xpath>
        </template>
    </data>
</odoo>
